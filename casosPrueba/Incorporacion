create

   int bot b,c,zeta,ene
    on activation:
      send.
    end
    on deactivation:
      collect.
    end
    on default:
      collect.
    end
   end

execute
    advance b.
    if true \/ false:
         deactivate c.
    else: 
         advance zeta.
    end


    while c <= 3:
        activate zeta.
    end
    
    create
       bool bot ene,z,i,j,juana
       end
    execute

       activate z.

       while i /= j :
           advance i.
           if ene=juana:
              activate i.
           end
       end

    end

end
